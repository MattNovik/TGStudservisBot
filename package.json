{
  "name": "studservis-bot",
  "version": "0.9.0",
  "description": "Studservis Bot for create orders",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint res/**/*.ts",
    "format": "eslint res/**/*.ts --fix",
    "build": "rimraf ./dist && babel --extensions .ts,.tsx res --out-dir dist && npm run sentry:sourcemaps",
    "dev": "tsnd --respawn res/bot.ts",
    "start:prod": "npm run build && pm2-runtime ecosystem.config.cjs",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org studservis --project tg-bot-creator ./ && sentry-cli sourcemaps upload --org studservis --project tg-bot-creator ./"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@sentry/cli": "^2.33.1",
    "@sentry/node": "^8.20.0",
    "@sentry/profiling-node": "^8.20.0",
    "axios": "^1.3.5",
    "js-md5": "^0.8.3",
    "node-fetch": "^3.3.2",
    "telegraf": "^4.11.2",
    "telegram-inline-calendar": "^1.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.24.9",
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-typescript": "^7.24.7",
    "@telegraf/types": "^7.1.0",
    "@types/node": "^20.14.12",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "pm2": "^5.4.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4"
  }
}
